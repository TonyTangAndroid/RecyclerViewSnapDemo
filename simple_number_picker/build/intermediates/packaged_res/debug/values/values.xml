<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="reference" name="SNPickerStyle"/>
    <string name="app_name">SimpleNumberPicker</string>
    <declare-styleable name="SNPicker">
        <attr format="color" name="snp_smallTextColor"/>
        <attr format="color" name="snp_bigTextColor"/>
        <attr format="dimension" name="snp_smallTextSize"/>
        <attr format="dimension" name="snp_bigTextSize"/>
        <attr format="string" name="snp_animationType"/>
        <attr format="dimension" name="snp_notchSize"/>
        <attr format="color" name="snp_notchColor"/>
        <attr format="integer" name="snp_delimNumber"/>
        <attr format="integer" name="snp_min"/>
        <attr format="integer" name="snp_max"/>
        <attr format="boolean" name="snp_vertical"/>
    </declare-styleable>
</resources>